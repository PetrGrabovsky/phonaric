    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
